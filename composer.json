{
  "name": "yiisoft/yii2-app-advanced",
  "description": "Yii 2 Advanced Application Template",
  "keywords": [
    "yii2",
    "framework",
    "advanced",
    "application template"
  ],
  "homepage": "http://www.yiiframework.com/",
  "type": "project",
  "license": "BSD-3-Clause",
  "repositories": [
    {
      "type": "composer",
      "url": "https://asset-packagist.org"
    }
  ],
  "support": {
    "issues": "https://github.com/yiisoft/yii2/issues?state=open",
    "forum": "http://www.yiiframework.com/forum/",
    "wiki": "http://www.yiiframework.com/wiki/",
    "irc": "irc://irc.freenode.net/yii",
    "source": "https://github.com/yiisoft/yii2"
  },
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-bootstrap": "*",
    "yiisoft/yii2-swiftmailer": "*",
    "dmstr/yii2-adminlte-asset": "^2.1",
    "mdmsoft/yii2-admin": "~2.0",
    "2amigos/yii2-chartjs-widget": "~2.0",
    "yiisoft/yii2-jui": "^2.0",
    "2amigos/yii2-date-picker-widget": "~1.0",
    "2amigos/yii2-date-time-picker-widget": "~1.0",
    "2amigos/yii2-ckeditor-widget": "^1.0@dev",
    "2amigos/yii2-tinymce-widget": "~1.1",
    "vilochane/yii2-barcode-generator": "dev-master",
    "2amigos/yii2-editable-widget": "~1.0",
    "kartik-v/yii2-grid": "dev-master",

    "kartik-v/yii2-widget-select2": "dev-master",
    "kartik-v/yii2-widget-fileinput": "dev-master",
    "liyunfang/yii2-widget-linkpager": "^1.0",
    "kartik-v/yii2-widget-activeform": "dev-master",
    "kartik-v/yii2-widget-typeahead": "*",
    "leandrogehlen/yii2-treegrid": "*",
    "2amigos/qrcode-library": "~1.1",
    "yiisoft/yii2-httpclient": "2.0.x-dev"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "yiisoft/yii2-debug": "*",
    "yiisoft/yii2-gii": "*",
    "yiisoft/yii2-faker": "*"
  },
  "config": {
    "process-timeout": 1800,
    "allow-plugins": {
      "yiisoft/yii2-composer": true
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
